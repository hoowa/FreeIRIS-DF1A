       cpu0-supply = <&rn5t567_core>;
       dcdc0-maxv = <1000000>;
};
*/


/* I2C0=20 21 启用PG1 */
&i2c0 {
	status = "okay";
	#address-cells = <1>;
	#size-cells = <0>;
/*	/include/	"sf16a18_pmu.dtsi"*/
};

/* I2C1=22 23 与UART2_RX TX冲突(PG1组)，关闭 */
&i2c1 {
	status = "disabled";
};

/* I2C2=24 25 与时钟、SYS_LED冲突，关闭 */
&i2c2 {
	status = "disabled";
};

&spi0 {
	status = "okay";
	//use-dma;
	dmas = <&gdma 10
	&gdma 11>;
	dma-names = "tx", "rx";

	#address-cells = <1>;
	#size-cells = <0>;

	w25q128@0 {
		compatible = "w25q128";
		reg = <0>;	/* chip select */
		spi-max-frequency = <33000000>;

		bank-width = <2>;
		device-width = <2>;
		#address-cells = <1>;
		#size-cells = <1>;

		partition@0 {
			label = "spl-loader";
			reg = <0x0 0x20000>; /* 128k */
			read-only;
		};

		partition@20000 {
			label = "u-boot";
			reg = <0x20000 0x60000>; /* 384k */
		};

		partition@80000 {
			label = "u-boot-env";
			reg = <0x80000 0x10000>; /* 64k */
		};

		factory:partition@90000 {
			label = "factory";
			reg = <0x90000 0x10000>; /* 64k */
		};

		partition@a0000 {
			label = "firmware";
			reg = <0xa0000 0xf60000>; /* 640k-16M */
		};
	};

	spidev: spi@8200000 {
		compatible = "rohm,dh2228fv";
		reg = <1>;
		clock = <50000000>;
		spi-cpha;
		spi-cpol;
		spi-max-frequency=<12000000>;
	};
};

/* 核心板调试接口 */
&uart0 {
	status = "okay";
};

/* PG2组，可选IIS0、PCM0、UART1默认启动了IIS0这里关闭 */
&uart1 {
	status = "disabled";
};

/* 这里启用22 23两个IO(PG1组)，24 25(与时钟、SYS_LED冲突，不启用) */
&uart2 {
	pinctrl-0 = <&uart2_tx &uart2_rx>;
	status = "okay";
};

/* 31 32用作RS485接口，33 34 35用作PG3的GPIO这里不启用 */
&uart3 {
	pinctrl-0 = <&uart3_tx &uart3_rx>;
	status = "okay";
};

/* 参考i2s0的说明，不知道为啥i2s和pcm0都一起启动了(PG2组) */
&pcm0 {
	status = "okay";
};

/* 尚未知晓 */
&pwm0 {
	status = "okay";
};

/* IO37接口PG3组 */
&pwm1 {
	status = "okay";
};

&timer0 {
	status = "okay";
};

&watchdog {
	status = "okay";
};

/* IO 35 62 (与PG3组冲突)，关闭 */
&spdif
{
	status = "disabled";
};

&wifi_rf {
	status = "okay";
	gpio-expa = <&gpio 51 0>;
	/*for new evb board,gpio use different map with that in p10h board*/
	expa_map_type = <2>;
};

&wifi_lb {
	status = "okay";

	#address-cells = <1>;
	#size-cells = <0>;
	gpio-leds = <&gpio 61 0>;
	smp-affinity = <2>;
};

&wifi_hb {
	status = "okay";

	#address-cells = <1>;
	#size-cells = <0>;
	gpio-leds = <&gpio 36 0>;
	smp-affinity = <3>;
};

&gpio_keys {
        status = "okay";

        hoowa@33 {
                label = "hoowa";
                /*linux,input-type = <0x5>;*/
                linux,code = <0x102>;
                gpios = <&gpio 33 1>;
                poll-interval = <10>;
                debounce-interval = <20>;
        };
};

&leds {
        status = "okay";
        sys_led {
                label = "sys_led";
                gpios = <&gpio 25 1>;
        };
};
