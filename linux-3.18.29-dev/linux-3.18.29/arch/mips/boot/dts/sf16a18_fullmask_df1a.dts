/dts-v1/;

#include "sf16a18_full_mask.dtsi"
#include "sf16a18-thermal.dtsi"


/ {
	#address-cells = <1>;
	#size-cells = <1>;
	compatible = "siflower,sf16a18-soc";

	/* ETH的LED灯尚未完成 */
	eth0-led {
		compatible = "eth-led";
		pinctrl-names = "default",  "gpio";
		pinctrl-0 = <&eth0_led>;
		pinctrl-1 = <&eth0_led_gpio>;
		eth0 {
			label = "eth_led0";
			gpios = <&gpio 55 1>;
			default-state = "on";
		};
	};

	eth1-led {
		compatible = "eth-led";
		pinctrl-names = "default",  "gpio";
		pinctrl-0 = <&eth1_led>;
		pinctrl-1 = <&eth1_led_gpio>;
		eth1 {
			label = "eth_led1";
			gpios = <&gpio 56 1>;
			default-state = "on";
		};
	};

	/* 我们没有SPDIF和ES7149声卡部分，尚未完成 */
	spdif_codec: spdif-codec@0 {
		compatible = "linux,spdif-dit";
	};

	codec_es7149: codec-es7149@0 {
		compatible = "es7149";
		speaker-gpio = <&gpio 45 0>;
		boot-gpio = <&gpio 53 0>;
		reset-gpio = <&gpio 54 0>;
	};

	/* ETH_LED部分尚未完成 */
	sf_mm_led: sf-mm-led {
		compatible = "sf-mm-led";
		blue-led-gpio = <&gpio 55 0>;
		green-led-gpio = <&gpio 56 0>;
		red-led-gpio = <&gpio 59 0>;
		default-mode = <0x39>;
		default-interval = <500>;
	};

	/* 不知道干嘛的，尚未完成 */
	w18_wifi_rf: w18_wifi-rf@7A00000{
		compatible = "siflower,pistachio-uccp";
		phy = <&phy1>;
		#address-cells = <1>;
		#size-cells = <0>;
		phy1: w18_wifi-phy@1 {
			reg = <1>;
			max-speed = <100>;
		};
	};

	/* 我们没有SPDIF和ES7149声卡部分，尚未完成 */
	spdif-audio@0 {
		compatible = "siflower,siflower-audio";
		model = "Siflower Spdif Audio";
		audio-controller = <&spdif>;
		audio-codec = <&spdif_codec>;
		audio-out;
	};

};

/* 启用USB PHY */
&usb_phy{
	status = "okay";
};

/* 芯片内置以太网划分 */
&ethernet {
	shutdown-portlist = "wll**";
        led-on-off-time = /bits/ 16 <0xff 0x2ff>;
	status = "okay";
	smp-affinity = <2>;
};

&switch{
	sfax8,port-map = "wll**";
	status = "okay";
	smp-affinity = <2>;
};

&gmac {
	status = "okay";
	smp-affinity = <0>;
};

/* 不知道干嘛的，尚未完成 */
&gdu {
	status = "okay";
	num-windows = <2>;
	rgb_order = <0 1 2>;
	power_gpio = <26 37>;
	bpp-mode = "RGB888";

	display-timings {
		native-mode = <&timing0>;
		timing0: gm05004001q {
			clock-frequency = <34539600>;
			hactive = <800>;
			vactive = <480>;
			hback-porch = <46>;
			hfront-porch = <210>;
			vback-porch = <23>;
			vfront-porch = <22>;
			hsync-len = <20>;
			vsync-len = <10>;
		};
	};
};

&sham {
	status = "okay";
};

&cipher {
	status = "okay";
};

&rng {
	status = "okay";
};

&gdma {
	status = "okay";
};

/* USB接口 */
&usb {
	status = "okay";
};

/* EMMC存储器，DF1A没有 */
&emmc {
	status = "disabled";
};

/* SD1接口，用作TF卡 */
&sdio {
	status = "okay";
};

/* IIS1=31 32 33 34 35，DF1A的RS485(31 32)/PG3(33 34 35)冲突 */
&i2s_master {
	status = "disabled";
};

/* IIS0=26 27 28 29 30，保留IIS0，可选定义为PCM0或全功能UART1 */
&i2s_slave {
	status = "okay";
};

/* This is used to set voltage during dvfs.
 * You should first know which pmu is in use, and
 * remove the default core-voltage set in pmu dts.
 */
/*
